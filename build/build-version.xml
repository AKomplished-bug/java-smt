<!-- vim: set tabstop=8 shiftwidth=4 expandtab filetype=ant : -->
<project name="version" basedir=".">

    <target name="determine-version">
        <exec executable="git"
                outputproperty="version"
                failonerror="true">
            <arg value="describe"/>
        </exec>
    </target>
</project>
